<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="regression suit" verbose="1">
    <groups>
        <run>
            <include name="regression"/>
        </run>
    </groups>
    <listeners>
        <listener class-name="com.nopcommerce.demo.customlisteners.CustomListeners"></listener>
        <listener class-name="org.uncommons.reportng.HTMLReporter"></listener>
        <listener class-name="org.uncommons.reportng.JUnitXMLReporter"></listener>
    </listeners>
    <test name="regression Test">
        <classes>
            <class name="com.nopcommerce.demo.testsuit.ComputerTest"></class>
            <class name="com.nopcommerce.demo.testsuit.ElectronicTest"></class>
            <class name="com.nopcommerce.demo.testsuit.LoginTest"></class>
            <class name="com.nopcommerce.demo.testsuit.RegisterMenuTest"></class>
            <class name="com.nopcommerce.demo.testsuit.TopMenuTest"></class>

        </classes>
    </test>
</suite>